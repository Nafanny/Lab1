<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="cord" class="Coords" scope="prototype">
        <property name="x" value="10"/>
        <property name="y" value="6"/>
    </bean>
    <bean id="figure" class="Shape">
        <constructor-arg value="red"/>
    </bean>
    <bean id="point" class="Point">
        <constructor-arg value="3"/>
        <constructor-arg value="1"/>
        <property name="X" value="11"/>
        <property name="Y" value="22"/>
    </bean>
    <bean id="circ" class="Circle">
        <constructor-arg value="7"/>
        <constructor-arg value="8"/>
        <constructor-arg value="16"/>
        <property name="radius" value="20"/>
    </bean>

</beans>